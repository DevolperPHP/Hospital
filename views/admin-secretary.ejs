<%- include("layout/header") -%>

<table class="doctor-enter-code-table table container">
  <thead>
    <tr>
      <th scope="col">اسم السكرتير</th>
      <th scope="col">اسم الطبيب</th>
      <th scope="col">تاريخ الولادة</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <% sec.forEach(data => { %>
      <tr>
      <th scope="row"><%= data.name%></th>
      <td><%= data.for_doctor%></td>
      <td><%= data.born%></td>
      <td><form action="/admin/secretary/<%= data.id%>/delete?_method=DELETE" method="POST">
          <button class="btn btn-danger">حذف</button>
      </form></td>
    </tr>
    <% }) %>
  </tbody>
</table>

<%- include("layout/footer") -%>